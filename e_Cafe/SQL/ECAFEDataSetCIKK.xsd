                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT     c.CIKK_ID, c.MEGNEVEZES, c.ERTEKESITES_TIPUSA, CASE WHEN CIKK_TIPUS = 1 THEN 'Összetett' ELSE 'Normál' END AS CIKK_TIPUS, 
